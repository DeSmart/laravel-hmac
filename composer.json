{
    "name": "desmart/php-hmac",
    "type": "library",
    "description": "Simple object wrapper for HMAC",
    "license": "MIT",
    "keywords": ["desmart", "php", "hmac"],
    "authors": [
        {
            "name": "Micha≈Ç Golon",
            "email": "gollum@desmart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "DeSmart\\HMAC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeSmart\\HMAC\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
